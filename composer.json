{
  "require": {
    "php": ">=8.2",
    "flightphp/container": "^1.3",
    "flightphp/core": "^3.17",
    "leafs/auth": "^4.2",
    "leafs/db": "^4.0",
    "leafs/form": "^3.2",
    "leafs/leaf": "^4.4",
    "league/oauth2-github": "^3.1",
    "league/oauth2-google": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "SIPAN\\": "app"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "scripts": {
    "dev": [
      "Composer\\Config::disableProcessTimeout",
      "composer setup",
      "php -S localhost:80"
    ],
    "setup": "php -r \"if (!file_exists('.env.php')) copy('.env.example.php', '.env.php');\"",
    "test": "phpunit",
    "rector": "rector"
  },
  "require-dev": {
    "guzzlehttp/guzzle": "^7.9",
    "phpunit/phpunit": "^11.5",
    "rector/rector": "^2.1",
    "symfony/var-dumper": "^7.3"
  },
  "config": {
    "sort-packages": true
  }
}
